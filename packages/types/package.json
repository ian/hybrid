{
	"name": "@hybrd/types",
	"version": "1.3.2",
	"keywords": ["xmtp", "agent", "ai", "hybrid", "types", "typescript"],
	"type": "module",
	"files": ["dist", "src"],
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"build": "tsup",
		"build:watch": "tsup --watch",
		"clean": "rm -rf .turbo dist",
		"test": "vitest",
		"test:run": "vitest run",
		"typecheck": "tsc --noEmit",
		"lint": "biome lint --unsafe",
		"lint:fix": "biome lint --write --unsafe",
		"format": "biome format --write"
	},
	"dependencies": {
		"ai": "^5.0.28",
		"zod": "^3.23.8"
	},
	"peerDependencies": {
		"@xmtp/content-type-group-updated": "*",
		"@xmtp/content-type-reaction": "*",
		"@xmtp/content-type-reply": "*",
		"@xmtp/content-type-transaction-reference": "*",
		"@xmtp/content-type-wallet-send-calls": "*",
		"@xmtp/node-sdk": "*",
		"hono": "*"
	},
	"devDependencies": {
		"@config/biome": "workspace:*",
		"@config/tsconfig": "workspace:*",
		"@types/node": "catalog:stack",
		"tsup": "^8.5.0",
		"vitest": "^3.2.4"
	}
}
