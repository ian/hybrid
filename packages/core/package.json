{
	"name": "hybrid",
	"version": "1.4.4",
	"keywords": [
		"xmtp",
		"agent",
		"ai",
		"hybrid",
		"blockchain",
		"messaging"
	],
	"type": "module",
	"files": [
		"dist",
		"src"
	],
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./behaviors": {
			"types": "./src/behaviors/index.ts",
			"import": "./dist/behaviors.js",
			"require": "./dist/behaviors.cjs"
		},
		"./tools": {
			"types": "./src/tools/index.ts",
			"import": "./dist/tools.js",
			"require": "./dist/tools.cjs"
		},
		"./types": {
			"types": "./src/types.ts",
			"import": "./dist/types.js",
			"require": "./dist/types.cjs"
		}
	},
	"scripts": {
		"build": "tsup",
		"build:watch": "tsup --watch",
		"clean": "rm -rf .turbo dist",
		"test": "vitest run",
		"test:watch": "vitest",
		"typecheck": "tsc --noEmit",
		"lint": "biome lint --unsafe",
		"lint:fix": "biome lint --write --unsafe",
		"format": "biome format --write"
	},
	"dependencies": {
		"@coinbase/agentkit": "^0.6.2",
		"@coinbase/agentkit-vercel-ai-sdk": "^0.1.0",
		"@coinbase/coinbase-sdk": "^0.22.0",
		"@hono/node-server": "^1.13.5",
		"@hybrd/ponder": "workspace:*",
		"@hybrd/types": "workspace:*",
		"@hybrd/utils": "workspace:*",
		"@hybrd/xmtp": "workspace:*",
		"ai": "^5.0.28",
		"eta": "^3.5.0",
		"hono": "^4.9.2",
		"jsonwebtoken": "^9.0.2",
		"uint8arrays": "^5.1.0",
		"viem": "^2.33.3",
		"wagmi": "^2.15.6",
		"zod": "catalog:stack"
	},
	"devDependencies": {
		"@config/biome": "workspace:*",
		"@config/tsconfig": "workspace:*",
		"@types/degit": "^2.8.4",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/node": "catalog:stack",
		"tsup": "^8.5.0",
		"vitest": "^3.2.4"
	}
}
